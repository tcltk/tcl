test lpop-6.1 {compiled: nested} -body {
    apply {l {
	set l2 $l
	list [lpop l 0 0 0 0] $l $l2
    }} "x y"
} -result {x {{{{}}} y} {x y}}
test lpop-6.2 {compiled: nested} -body {
    apply {l {
	list [lpop l 0 1] $l
    }} "{x y} {a b}"
} -result {y {x {a b}}}
test lpop-6.3 {compiled: nested} -body {
    apply {l {
	list [lpop l 1 0] $l
    }} "{x y} {a b}"
