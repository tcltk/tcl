test lpop-5.1 {compiled: basic functionality} -body {
    apply {l {
	list [lpop l 0] $l
    }} "x y z"
} -result {x {y z}}
test lpop-5.2 {compiled: basic functionality} -body {
    apply {l {
	list [lpop l 1] $l
    }} "x y z"
} -result {y {x z}}
test lpop-5.3 {compiled: basic functionality} -body {
    apply {l {
	list [lpop l] $l
    }} "x y z"
} -result {z {x y}}
